<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="mssp-entity" transaction-type="RESOURCE_LOCAL">
    <provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
		<class>com.mmd.mssp.domain.CommArea</class>
		<class>com.mmd.mssp.domain.ProductSeries</class>
		<class>com.mmd.mssp.domain.Pannel</class>
		<class>com.mmd.mssp.domain.CommSeries</class>
		<class>com.mmd.mssp.domain.Product</class>
		<class>com.mmd.mssp.domain.CommArea</class>
		<class>com.mmd.mssp.domain.CommCity</class>
		<class>com.mmd.mssp.domain.PsiReason</class>
		<class>com.mmd.mssp.domain.CommAgent</class>
		<class>com.mmd.mssp.domain.CommRole</class>
		<class>com.mmd.mssp.domain.PsiInventory</class>
		<class>com.mmd.mssp.domain.PsiSellIn</class>
		<class>com.mmd.mssp.domain.PsiSellInDatasource</class>
		<class>com.mmd.mssp.domain.PsiEstimateByProduct</class>
		<class>com.mmd.mssp.domain.PsiSellOut</class>
		<class>com.mmd.mssp.domain.Vacation</class>
	<properties>
      <!-- <property name="javax.persistence.jdbc.driver" value="net.sourceforge.jtds.jdbc.Driver"/>
